<?php

/**
 * @file
 * Contains custom_graphql_mutation.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function custom_graphql_mutation_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the custom_graphql_mutation module.
    case 'help.page.custom_graphql_mutation':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('A custom GraphQL Mutation Schema &amp; Plugin') . '</p>';
      return $output;

    default:
  }
}
